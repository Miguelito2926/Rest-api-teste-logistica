import com.ed.edlog.api.domain.model.Cliente;

@RestController
public class ClienteController {
	
	@GetMapping ("/clientes")
	public List<Cliente> listar() {
		Cliente cliente1 = new Cliente();
		cliente1.setId(1L); 
		cliente1.setNome("Ednaldo");
		cliente1.setTelefone("21 97777777");
		
		return null;
	}

}
